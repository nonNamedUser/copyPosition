local l=l or 2 if l == 1 then for k,v in pairs(getgenv()) do print(k,v) end else local e=getgenv();local function d(t,i,s)i=i or"";s=s or{};if s[t]then print(i.."*cycle*")return end;s[t]=true;for k,v in pairs(t)do local key="["..tostring(k).."]";if type(v)=="table"then print(i..key.." = {");d(v,i.."    ",s);print(i.."}")else print(i..key.." = "..tostring(v).." ("..type(v)..")")end end end;print("=== getgenv() dump start ===");d(e);print("=== getgenv() dump end ===") end
